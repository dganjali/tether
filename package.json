{
  "name": "shelter-monitoring-system",
  "version": "1.0.0",
  "description": "Toronto Shelter Analytics & Prediction System",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "build": "./build.sh",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "shelter",
    "analytics",
    "prediction",
    "react",
    "express",
    "tensorflow"
  ],
  "author": "",
  "license": "MIT"
} 